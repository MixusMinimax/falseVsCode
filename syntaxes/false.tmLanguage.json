{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "FALSE",
	"patterns": [
		{
			"include": "#characters"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#references"
		},
		{
			"include": "#commands"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"begin": "\\{",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.comment.false"
						}
					},
					"end": "\\}",
					"name": "comment.block.empty.false"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.false",
					"match": "\\b(if|while|for|return|cock)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.false",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.false",
					"match": "\\\\."
				}
			]
		},
		"numbers": {
			"patterns": [
				{
					"name": "keyword.other.unit.suffix.integer.false",
					"match": "[0-9]+"
				}
			]
		},
		"operators": {
			"patterns": [
				{
					"name": "keyword.operator.arithmetic.false",
					"match": "\\+|-|\\*|/|_"
				},
				{
					"name": "keyword.operator.logical.false",
					"match": "\\||&|~"
				},
				{
					"name": "keyword.operator.comparison.false",
					"match": ">|="
				}
			]
		},
		"references": {
			"patterns": [
				{
					"name": "entity.name.selector.false",
					"match": "[a-z]"
				}
			]
		},
		"commands": {
			"patterns": [
				{
					"name": "entity.name.function.false",
					"match": "\\.|,|\\\\|@|:|;|!|\\?|Ã¸|\\$|%|#"
				}
			]
		},
		"characters": {
			"patterns": [
				{
					"name": "string.quoted.single.false",
					"begin": "'",
					"end": "[^']"
				}
			]
		}
	},
	"scopeName": "source.f"
}